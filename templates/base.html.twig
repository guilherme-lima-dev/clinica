                                       class="p-0 btn">
                                        <div class="widget-heading">
                                            {{ app.user.funcionarios ? app.user.funcionarios.nome : app.user.pacientes.nome }}
                                        </div>
                                        <div class="widget-subheading">
                                            {{ app.user.perfil.nome }}
                                        </div>
                                    </a>
