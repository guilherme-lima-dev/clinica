<script>
    $(document).ready(function () {
        $('.datepicker').datepicker({
            format: "dd/mm/yyyy",
            todayBtn: "linked",
            clearBtn: true,
            language: "pt-BR",
            autoclose: true,
            todayHighlight: true
        });
        $('.datetimepicker').datetimepicker();
        $('.clockpicker').clockpicker({
            donetext: 'Fechar',
            default: 'now',
        });
        $('.select2').select2({
            width: 'resolve',
            language: 'pt-BR',
            theme: 'bootstrap',
            placeholder: function () {
                $(this).data('placeholder');
            }
        });
    });
</script>
