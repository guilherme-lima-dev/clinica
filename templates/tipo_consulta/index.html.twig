    <table class="table">
        <thead>
            <tr>
                <th>IdtipoConsulta</th>
                <th>Nome</th>
                <th>Descricao</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tipo_consultum in tipo_consultas %}
            <tr>
